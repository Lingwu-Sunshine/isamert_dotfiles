[Desktop Entry]
Type=Application
Name=Shortcut to
Terminal=false
Exec=st
Actions=walls;fkill;phonepics;screenshots

[Desktop Action walls]
Name=Wallpapers
Exec=/bin/sh -c "sxiv -t ~/Pictures/wallpaper"

[Desktop Action phonepics]
Name=Phone pictures
Exec=/bin/sh -c "sxiv -t ~/Pictures/phone/Camera"

[Desktop Action screenshots]
Name=Screenshots
Exec=/bin/sh -c "sxiv -t ~/Pictures/screenshots"

[Desktop Action fkill]
Name=Fuzzy kill (fkill)
Exec=/bin/sh -c "st -c float -g 150x20 -e fuzzy kill"
