# After adding a new calendar/contacts group, run this to discover them:
# vdirsyncer discover calendar

[general]
status_path = "~/.dav/cache/vdir-status/"

[pair contacts]
a = "contacts_local"
b = "contacts_remote"
collections = ["from a", "from b"]

[storage contacts_local]
type = "filesystem"
fileext = ".vcf"
path = "~/.dav/contacts"

[storage contacts_remote]
type = "carddav"
auth= "digest"
url = "http://galileo/baikal/html/dav.php/"
username = "isamert"
password.fetch = ["command", "getpassword", "/cloud/baikal"]



[pair calendar]
a = "calendar_local"
b = "calendar_remote"
collections = ["from a", "from b"]

[storage calendar_local]
type = "filesystem"
fileext = ".ics"
path = "~/.dav/calendars/"

[storage calendar_remote]
type = "caldav"
auth= "digest"
url = "http://galileo/baikal/html/cal.php/calendars/isamert/"
username = "isamert"
password.fetch = ["command", "getpassword", "/cloud/baikal"]
